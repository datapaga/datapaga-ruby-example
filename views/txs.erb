
<p>Transactions list</p>
    <table class="table">
    <thead>
      <tr>
        <th scope="col">Cantidad</th>
        <th scope="col">Cliente</th>
        <th scope="col">Estado</th>
        <th scope="col">Fecha de Transacci√≥n</th>
        <th scope="col">Detalles</th>
      </tr>
    </thead>
    <tbody>
      <% TXs.each do |tx|  %>
        <tr>
          <td><%= tx['total_amount'] %></td>
          <td><%= tx['client_email'] %></td>
          <td><%= tx['payment_status'] %></td>
          <td><%= tx['created_date'] %></td>
          <td><a href="<%= url("/txs/#{tx['id']}") %>" class="btn btn-primary">Ver</a></td>
        </tr>
      <% end %>
    </tbody>
  </table>
